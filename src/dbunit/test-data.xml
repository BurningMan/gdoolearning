<?xml version="1.0" encoding="UTF-8"?>
<dataset>
    <table name="student">
        <column>NAME</column>
    </table>
    <table name="subject">
        <column>NAME</column>

    </table>
    <table name="course">
        <column>SUBJECT_ID</column>
        <column>START_DATE</column>
        <column>FINISH_DATE</column>
    </table>

    <table name="submission">
        <column>STUDENT_ID</column>
        <column>COURSE_ID</column>
    </table>

    <table name="lesson">
        <column>COURSE_ID</column>
        <column>START_TIME</column>
        <column>TOPIC</column>
    </table>

    <table name="attendance">
        <column>STUDENT_ID</column>
        <column>LESSON_ID</column>
    </table>
</dataset>